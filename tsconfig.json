{
  "compileOnSave": false,
  "compilerOptions": {
    "module": "es2015",
    "target": "es5",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "sourceMap": true,
    "baseUrl": "ui/src",
    //"outDir": "./dist",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es5",
      "es2015.collection",
      "es6",
      "dom"
    ],
    "types": [
      "node",
      "core-js"
    ]
  },
  "awesomeTypeScriptLoaderOptions": {
    "useWebpackText": true
  }
}
